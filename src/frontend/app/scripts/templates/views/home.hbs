<div class="container-fluid">
  <section>
    <div class="page-header">
      <h1>Dashboard</h1>
    </div>
    
    <div class="row-fluid">
      <!-- Today start -->
      <div class="span12 m-widget">
        <div class="m-widget-header">
          <h3>Stats</h3>
        </div>

        <div class="m-widget-body">
          <div class="row-fluid">
            <div class="span2 m-stats-item">
              <span class="m-stats-val">{{facility}}</span>
              Facilities
            </div>

            <div class="span2 m-stats-item">
              <span class="m-stats-val">{{classes}}</span>
              Classes
            </div>

            <div class="span2 m-stats-item">
              <span class="m-stats-val">{{events}}</span>
              Events
            </div>

            <div class="span2 m-stats-item">
              <span class="m-stats-val">{{video}}</span>
              Videos
            </div>     

            <div class="span2 m-stats-item">
              <span class="m-stats-val">{{photo}}</span>
              Images
            </div>     
          </div>
        </div>
      </div>
    </div>
  </section>
</div>